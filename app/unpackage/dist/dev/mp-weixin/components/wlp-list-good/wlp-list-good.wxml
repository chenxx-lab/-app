<view class="{{['listGood',(type=='row')?'listGoodRow':'']}}" style="{{'width:'+(resWidth)+';'}}"><view data-event-opts="{{[['tap',[['_click',['img']]]]]}}" class="listGood__left" bindtap="__e"><view class="listGood__label">{{data.label}}</view><image class="listGood__img" lazy-load="{{true}}" src="{{data.imgUrl}}" mode="aspectFill"></image></view><view class="listGood__content"><text data-event-opts="{{[['tap',[['_click',['title']]]]]}}" class="listGood__title" bindtap="__e">{{data.title}}</text><block wx:if="{{data.titleSub}}"><text data-event-opts="{{[['tap',[['_click',['titleSub']]]]]}}" class="listGood__titleSub" bindtap="__e">{{data.titleSub}}</text></block><block wx:if="{{data.sku!=null}}"><view data-event-opts="{{[['tap',[['_click',['sku']]]]]}}" class="listGood__sku" bindtap="__e">{{''+data.sku[0].name+''}}<block wx:if="{{data.sku.length>1}}"><uni-icon vue-id="1414fde8-1" type="arrowdown" size="12" color="#fff" bind:__l="__l"></uni-icon></block></view></block><slot></slot></view><view class="listGood__bottom"><view class="listGood__price">{{''+data.price+''}}<block wx:if="{{data.originalPrice}}"><text class="listGood__price--original">{{data.originalPrice}}</text></block></view><block wx:if="{{inputValue!=null}}"><view class="listGoodNumbox" style="{{'padding-bottom:'+(type=='column'?'0':'8px')+';'}}"><block wx:if="{{data.sku!=null&&data.sku.length>1||inputValue==0}}"><view data-event-opts="{{[['tap',[['_calcValue',['add']]]]]}}" class="listGoodNumbox__empty" bindtap="__e"><uni-icon vue-id="1414fde8-2" size="20" type="plusempty" color="#fff" bind:__l="__l"></uni-icon>购物车<block wx:if="{{inputValue>0}}"><uni-badge vue-id="1414fde8-3" text="{{inputValue}}" type="error" bind:__l="__l" vue-slots="{{['default']}}">{{''+inputValue+''}}</uni-badge></block></view></block><block wx:else><block wx:if="{{inputValue>0}}"><view data-event-opts="{{[['tap',[['_calcValue',['subtract']]]]]}}" class="listGoodNumbox__minus" bindtap="__e">-</view><input class="listGoodNumbox__value" type="number" disabled="{{disabled}}" data-event-opts="{{[['blur',[['_onBlur',['$event']]]]]}}" value="{{inputValue}}" bindblur="__e"/><view data-event-opts="{{[['tap',[['_calcValue',['add']]]]]}}" class="{{['listGoodNumbox__plus',(disableAdd)?'listGoodNumbox__disabled':'']}}" bindtap="__e">+</view></block><block wx:else><view data-event-opts="{{[['tap',[['_click',['numbox']]]]]}}" class="{{['listGoodNumbox__helper',(inputValue<-1)?'listGoodNumbox__helper--error':'']}}" bindtap="__e"><slot name="numbox"></slot></view></block></block></view></block></view></view>